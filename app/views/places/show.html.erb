<h1><%= @place.name %> </h1>
<h2>Entries</h2>
<ul>
<% for entry in @entries %>
<li>
<strong><%= entry.title %> </strong> - <%=- entry.posted_on %><br>
<%= entry.description %>
</li>
<% end %>
</ul>

<%= link_to "Add new entry", "/places/#{@place.id}/entries/new" %> <br>
<%= link_to "back to all places", "/places" %>